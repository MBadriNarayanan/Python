    "a = -1\n",
    "\n",
    "b = 1\n",
    "\n",
    "c = 0\n",
    "\n",
    "c = a + b\n",
    "\n",
    "num = int(input(\" Enter Limit : \"))\n",
    "\n",
    "print(\" Fibonacci Series Till \",num,\" Is\")\n",
    "\n",
    "while(c < num):\n",
    "    \n",
    "    print(\" \",c,end = \" \")\n",
    "    \n",
    "    a = b\n",
    "    \n",
    "    b = c\n",
    "    \n",
    "    c = a + b"
