   "source": [
    "def factorial(n) :\n",
    "    \n",
    "    if n == 1 :\n",
    "        \n",
    "        return 1\n",
    "    \n",
    "    else :\n",
    "        \n",
    "        return n * factorial(n - 1)"
   ]
  },
